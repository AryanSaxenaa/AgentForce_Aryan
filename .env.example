# AI Request Settings
AI_MAX_TOKENS=1000
AI_TEMPERATURE=0.3
AI_TIMEOUT=30

# GitHub Integration (optional)
GITHUB_TOKEN=your_github_token_here

# Logging
LOG_LEVEL=INFO